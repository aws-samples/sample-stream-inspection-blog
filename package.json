{
  "name": "stream-intelligence-blog",
  "version": "0.1.0",
  "bin": {
    "stream-intelligence-blog": "bin/stream-intelligence-blog.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --passWithNoTests",
    "lint": "eslint lib/**/*.ts bin/**/*.ts --fix",
    "format": "prettier --write lib/**/*.ts bin/**/*.ts",
    "stream:start": "npx ts-node scripts/operations/stream-manager.ts start",
    "stream:stop": "npx ts-node scripts/operations/stream-manager.ts stop",
    "stream:status": "npx ts-node scripts/operations/stream-manager.ts status",
    "stream:generate": "npx ts-node scripts/operations/test-stream.ts",
    "cleanup:residual": "./scripts/operations/cleanup-residual.sh",
    "verify:srt": "ffmpeg -protocols | grep srt && echo '✅ SRT protocol supported' || echo '❌ SRT protocol not found'",
    "clean": "rm -rf cdk.out node_modules/.cache"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.16.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "aws-cdk": "^2.204.0",
    "eslint": "^9.30.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-auto-scaling": "^3.844.0",
    "@aws-sdk/client-cloudformation": "^3.700.0",
    "@aws-sdk/client-cloudwatch": "^3.844.0",
    "@aws-sdk/client-ec2": "^3.844.0",
    "@aws-sdk/client-mediaconnect": "^3.700.0",
    "@aws-sdk/client-medialive": "^3.844.0",
    "@aws-sdk/client-mediapackagev2": "^3.700.0",
    "aws-cdk-lib": "^2.204.0",
    "cdk-nag": "^2.36.33",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "streaming",
    "mediaconnect",
    "medialive",
    "gateway-load-balancer",
    "traffic-inspection",
    "security",
    "infrastructure-as-code"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/stream-intelligence-blog"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "npm:@isaacs/inflight@^1.0.6",
    "querystring": "^0.2.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/cdk.out/",
      "<rootDir>/node_modules/"
    ]
  }
}
