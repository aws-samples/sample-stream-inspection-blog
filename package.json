{
  "name": "stream-intelligence-blog",
  "version": "0.1.0",
  "bin": {
    "stream-intelligence-blog": "bin/stream-intelligence-blog.js"
  },
  "scripts": {
    "build": "tsc",
    "build:lambdas": "cd lib/custom-resources/mediapackage-ingest-urls/lambda && npm install && npm run build",
    "watch": "tsc -w",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "lint": "eslint lib/**/*.ts bin/**/*.ts --fix",
    "lint:check": "eslint lib/**/*.ts bin/**/*.ts",
    "security:check": "./scripts/security-review.sh",
    "security:quick": "npm run build && npx cdk synth --quiet 2>&1 | grep -E '(ERROR|WARN)' || echo 'âœ… No security issues found'",
    "deploy:secure": "npm run security:check && npx cdk deploy",
    "format": "prettier --write lib/**/*.ts bin/**/*.ts",
    "format:check": "prettier --check lib/**/*.ts bin/**/*.ts",
    "cdk": "cdk",
    "troubleshoot": "scripts/troubleshoot-deployment.sh",
    "get-ip": "node scripts/get-ip.js",

    "deploy:prod": "npx cdk deploy --require-approval never --outputs-file deployment-outputs.json",
    "destroy": "npx cdk destroy",
    "cleanup:residual": "./scripts/operations/cleanup-residual.sh",
    "cleanup:verify": "./scripts/operations/verify-cleanup.sh",
    "diff": "npx cdk diff",
    "stream:start": "npx ts-node scripts/operations/stream-manager.ts start",
    "stream:stop": "npx ts-node scripts/operations/stream-manager.ts stop",
    "stream:status": "npx ts-node scripts/operations/stream-manager.ts status",
    "stream:restart": "npx ts-node scripts/operations/stream-manager.ts restart",
    "stream:control": "./scripts/operations/stream-control.sh",
    "stream:test": "npx ts-node scripts/tests/test-stream-manager.ts",
    "stream:test-fixes": "node scripts/tests/test-stream-manager-fixes.js",
    "stream:generate": "npx ts-node scripts/operations/test-stream.ts",
    "stream:generate:broadcast": "./scripts/operations/run-test-stream.sh",
    "latency:test": "node scripts/tests/latency-test.js",
    "latency:quick": "./scripts/tests/run-latency-test.sh --profile quick",
    "latency:standard": "./scripts/tests/run-latency-test.sh --profile standard",
    "latency:extended": "./scripts/tests/run-latency-test.sh --profile extended",
    "latency:stress": "./scripts/tests/run-latency-test.sh --profile stress",
    "latency:help": "./scripts/tests/run-latency-test.sh --help",
    "latency:analyze": "node scripts/utilities/analyze-latency-results.js",
    "latency:compare": "node scripts/utilities/analyze-latency-results.js --compare",
    "keypair:download": "./scripts/operations/download-keypair.sh",
    "keypair:get": "npx ts-node scripts/operations/download-keypair.ts",
    "verify:ffmpeg": "./scripts/utilities/verify-ffmpeg.sh",
    "vpc:update": "./scripts/operations/update-vpc-interface.sh",
    "vpc:update-ip": "node scripts/operations/update-vpc-interface.js",
    "synth": "npx cdk synth",
    "doctor": "npx cdk doctor",
    "clean": "rm -rf cdk.out node_modules/.cache",
    "update": "npm update && npm audit fix",
    "check-updates": "npm outdated"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^22.16.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "aws-cdk": "^2.1020.2",
    "eslint": "^9.30.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.700.0",
    "@aws-sdk/client-mediaconnect": "^3.700.0",
    "aws-cdk-lib": "^2.202.0",
    "cdk-nag": "^2.36.33",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "streaming",
    "mediaconnect",
    "medialive",
    "gateway-load-balancer",
    "traffic-inspection",
    "security",
    "infrastructure-as-code"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/stream-intelligence-blog"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
